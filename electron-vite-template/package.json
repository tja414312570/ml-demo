{
  "name": "electron-vite-template",
  "version": "1.0.0",
  "main": "./dist/electron/main/main.js",
  "author": "sky <https://github.com/umbrella22>",
  "description": "electron-vite-template project",
  "license": "MIT",
  "scripts": {
    "dev": "tsx .electron-vite/dev-runner.ts",
    "build": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts -m prod && electron-builder -c build.json",
    "build:win32": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --win  --ia32",
    "build:win64": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --win  --x64 --dir",
    "build:mac": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --mac",
    "build:dir": "cross-env BUILD_TARGET=clean tsx .electron-vite/build.ts  && electron-builder -c build.json --dir",
    "build:clean": "cross-env BUILD_TARGET=onlyClean tsx .electron-vite/build.ts",
    "build:web": "cross-env BUILD_TARGET=web tsx .electron-vite/build.ts",
    "pack:resources": "tsx .electron-vite/hot-updater.ts",
    "dep:upgrade": "yarn upgrade-interactive --latest",
    "postinstall": "electron-builder install-app-deps",
    "test": "tsx .electron-vite/test-runner.ts",
    "plugin": "tsx .electron-vite/build-plugins.ts"
  },
  "dependencies": {
    "@babel/core": "^7.25.7",
    "@guolao/vue-monaco-editor": "^1.5.4",
    "@imengyu/vue3-context-menu": "^1.4.3",
    "@mdi/font": "^7.4.47",
    "@mdi/js": "^7.4.47",
    "@nut-tree/nut-js": "^3.1.2",
    "ansi-up": "^1.0.0",
    "axios": "^1.7.7",
    "blessed": "^0.1.81",
    "diff": "^7.0.0",
    "dockview-core": "^1.17.1",
    "dockview-vue": "^1.17.1",
    "electron-log": "^5.2.0",
    "electron-updater": "^6.3.4",
    "express": "^4.21.0",
    "fastify": "^5.0.0",
    "glob": "^11.0.0",
    "global-agent": "^3.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "node-pty": "file://../node-pty",
    "portscanner": "^2.2.0",
    "semver": "^7.6.3",
    "splitpanes": "^3.1.5",
    "term.js": "^0.0.7",
    "uuid": "^10.0.0",
    "vue-code-layout": "^1.0.7",
    "vue3-resizable": "^1.0.0-alpha.3",
    "vue3-splitpane": "^1.0.3",
    "vuetify": "^3.7.0-beta.1",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.25.7",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-replace": "^6.0.1",
    "@types/fs-extra": "^11.0.4",
    "@types/global-agent": "^2.1.3",
    "@types/lodash": "^4.17.12",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.7.2",
    "@types/portscanner": "^2.1.4",
    "@types/splitpanes": "^2.2.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "adm-zip": "^0.5.16",
    "cfonts": "^3.3.0",
    "chalk": "5.3.0",
    "cross-env": "^7.0.3",
    "del": "^7.1.0",
    "dotenv": "^16.4.5",
    "electron": "^32.1.2",
    "electron-builder": "^25.0.5",
    "electron-devtools-vendor": "^3.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "http-mitm-proxy": "^1.1.0",
    "inquirer": "^11.0.2",
    "javascript-obfuscator": "^4.1.1",
    "listr2": "^8.2.4",
    "minimist": "^1.2.8",
    "pinia": "^2.2.2",
    "portfinder": "^1.0.32",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-obfuscator": "^1.1.0",
    "sass": "^1.79.3",
    "tslib": "^2.7.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue": "^3.5.8",
